<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#342B49"

    >

    <ImageButton
        android:id="@+id/upload_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignBottom="@id/upload_heading"
        android:layout_centerVertical="true"
        android:layout_marginBottom="21dp"
        app:srcCompat="?attr/homeAsUpIndicator" />

    <TextView
        android:id="@+id/upload_heading"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/upload_back_btn"
        android:fontFamily="casual"
        android:gravity="center"
        android:text="All Photos"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="25sp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="569dp"
        android:layout_below="@+id/upload_back_btn"
        android:layout_marginBottom="80dp"
        android:background="#77F1E8FF">

        <TableLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/upload_photo_1"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_2"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_3"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_4"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_5"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/upload_photo_6"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_7"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_8"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_9"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_10"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/upload_photo_11"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_12"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_13"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>

                <ImageView
                    android:id="@+id/upload_photo_14"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>
                <ImageView
                    android:id="@+id/upload_photo_15"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/qr_code_with_bg"
                    android:layout_margin="6dp"/>


            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">


            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </TableRow>

        </TableLayout>


    </RelativeLayout>

    <Button
        android:id="@+id/upload_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="15dp"
        android:background="#77F1E8FF"
        android:text="Upload"
        />

</RelativeLayout>
